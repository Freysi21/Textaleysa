@model IEnumerable<Textaleysa.Models.ViewModel.ListRequestViewModel>

<p>
    @Html.ActionLink("Create New", "CreateRequest")
</p>
<table class="table">
    <tr>
        <th>
            Fjöldi atkvæða
        </th>
        <th>
            @Html.DisplayNameFor(model => model.mediaTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.language)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.userName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            <span class="qty">
            </span>
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.mediaTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.language)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.userName)
        </td>
        <td>
        <a id="vote-request" href="#!"><span class="glyphicon glyphicon-thumbs-up"/></a>
        </td>
    </tr>
}

</table>
